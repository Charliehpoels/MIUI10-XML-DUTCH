  <string name="dummy_fs">precision mediump float; uniform mat4 mg_TextureMatrix; uniform sampler2D mg_Texture0, mg_Texture1; uniform vec4 mg_Color; uniform float mg_Texture0_Weight, mg_Texture1_Weight, mg_Color_Weight; uniform vec4 mg_Texture0_Modulate, mg_Texture1_Modulate; uniform bool mg_Transparent_Title_Enabled; uniform vec2 mg_Transparent_Title; uniform sampler2D mg_Transparent_Title_Mask; varying vec2 mg_UV0, mg_UV1; vec2 transform_uv(vec2 uv) { vec4 v = vec4(uv, 0, 1); v = mg_TextureMatrix * v; return v.xy; } void main() { vec4 col = vec4(0.0); float totalWeight = 0.0; if(mg_Texture0_Weight &gt; 0.05) { col += mg_Texture0_Modulate * texture2D(mg_Texture0, transform_uv(mg_UV0)) * mg_Texture0_Weight; totalWeight += mg_Texture0_Weight; } if(mg_Texture1_Weight &gt; 0.05) { col += mg_Texture1_Modulate * texture2D(mg_Texture1, transform_uv(mg_UV1)) * mg_Texture1_Weight; totalWeight += mg_Texture1_Weight; } if(mg_Color_Weight &gt; 0.05) { col += mg_Color * mg_Color_Weight; totalWeight += mg_Color_Weight; } if (totalWeight &lt; 0.05) { discard; } col /= totalWeight; if(mg_Transparent_Title_Enabled) { float y = gl_FragCoord.y; if (y &gt;= mg_Transparent_Title[1]) { discard; } else if (y &gt;= mg_Transparent_Title[0]) { float total = mg_Transparent_Title[1] - mg_Transparent_Title[0]; float toTop = mg_Transparent_Title[1] - y; float scale = toTop / total; float alpha = 1.0 - length(texture2D(mg_Transparent_Title_Mask, vec2(0.0, scale))); col.a *= alpha; } } if(col.a &lt; 0.05) { discard; } gl_FragColor = col; }</string>
  <string name="dummy_vs">uniform mat4 mg_ModelView, mg_Projection; attribute vec2 mg_TexCoord0, mg_TexCoord1; attribute vec3 mg_Vertex; varying vec2 mg_UV0, mg_UV1; void main() { gl_Position = mg_Projection * mg_ModelView * vec4(mg_Vertex, 1); mg_UV0 = mg_TexCoord0; mg_UV1 = mg_TexCoord1; }</string>
